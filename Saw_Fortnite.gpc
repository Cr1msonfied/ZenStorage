  function GetSteadyAim(Axis, _Resolution) {
	if (Axis >= 0)
	{
		ReT = (Axis % _Resolution);

		if (ReT >= (_Resolution / 2))
			Axis = Axis + (_Resolution - ReT);
		else
			Axis -= ReT;
	}
	else
	{
		ReT = (Axis % _Resolution);

		if (abs(ReT) >= (_Resolution / 2))
			Axis = Axis - (_Resolution + ReT);
		else
			Axis += abs(ReT);
	}
		return Axis;
}

