int _angle,_mangle,x,y,mx,my;
function aa(stick, shape, size) {
    _angle += velcro[0 + (2 * aa_when())];
    if(!shape) {
   		x = polar(size * (1 - (_angle % 90) / 90));
    	y = polar(size * (1 - (_angle % 180) / 180));
		
		if(_angle % 360 > 90 && _angle % 360 < 270)
 	       	x = -x;
    	if(_angle % 360 > 180)
        	y = -y;
	} else if(shape == 1) {
        x = polar(size * (1 - abs((_angle % 360) - 180) / 180));
    	y = polar(size * (1 - abs(_angle % 360) / 180));
   		
   		if((_angle % 360) > 180)
    	    x = -x;
    	if(_angle % 360 > 270 || _angle % 360 < 90)
    	    y = -y;
	} else if(shape == 2) {
		x = polar(size * (1 - abs((_angle % 360) - 180) / 180));
    	y = polar(size * (1 - abs(_angle % 360) / 180));
    
    	if((_angle % 360) > 225 && (_angle % 360) < 315)
        	x = -x;
    	if((_angle % 360) > 45 && (_angle % 360) < 135)
        	x = -x;
    	if((_angle % 360) > 135 && (_angle % 360) < 225)
        	y = -y;
    	if((_angle % 360) > 315 || (_angle % 360) < 45)
        	y = -y;
	}
    offset(stick,x);
    offset(stick + 1,y);
}
function motion_aa(stick, shape, size) {
    _mangle += motion[0 + (2 * aa_when())];
    if(!shape) {
   		mx = polar(size * (1 - (_mangle % 90) / 90));
    	my = polar(size * (1 - (_mangle % 180) / 180));
		
		if(_mangle % 360 > 90 && _mangle % 360 < 270)
 	       	mx = -mx;
    	if(_mangle % 360 > 180)
        	my = -my;
	} else if(shape == 1) {
        mx = polar(size * (1 - abs((_mangle % 360) - 180) / 180));
    	my = polar(size * (1 - abs(_mangle % 360) / 180));
   		
   		if((_mangle % 360) > 180)
    	    mx = -mx;
    	if(_mangle % 360 > 270 || _mangle % 360 < 90)
    	    my = -my;
	} else if(shape == 2) {
		mx = polar(size * (1 - abs((_mangle % 360) - 180) / 180));
    	my = polar(size * (1 - abs(_mangle % 360) / 180));
    
    	if((_mangle % 360) > 225 && (_mangle % 360) < 315)
        	mx = -mx;
    	if((_mangle % 360) > 45 && (_mangle % 360) < 135)
        	mx = -mx;
    	if((_mangle % 360) > 135 && (_mangle % 360) < 225)
        	my = -my;
    	if((_mangle % 360) > 315 || (_mangle % 360) < 45)
        	my = -my;
	}
    offset(stick,mx);
    offset(stick + 1,my);
}
function reduce(x) {
	if(velcro_vision == 2) {
