function singleButtonActivation(){  
	for (i = 0; i < sizeof(PressBtns); i++) {
		if(get_ival(PressBtns[i]) && get_ptime(PressBtns[i]) > 400 && keep[time_cpu] < 8){
    		return i;
    	}     		
    }
    return NOT_USE;
}
int list_count,list_size,list_offset,mod_limit,mod_list_offset, List_Offset;
function find_list(){
	list_count = 0;
	list_size = 0;
	list_offset = 0;
	for(j = 1; j <= sizeof(LIST_LIMITS); j++){
		if(mod > list_count){
			list_offset++;
		}
		if(!LIST_LIMITS[j]){
			list_count++;
		}
		if(list_count == mod && LIST_LIMITS[j]){
			list_size++;
		}
		mod_list_offset = list_offset - mod;
		mod_limit = list_size - 1;
		List_Offset = list_offset + 1;
	}
}
int Font;
function x_location(f_chars,f_font,f_location) {

	if(f_font == OLED_FONT_SMALL) {Font = OLED_FONT_SMALL_WIDTH;}	    
    if(f_font == OLED_FONT_MEDIUM){Font = OLED_FONT_MEDIUM_WIDTH;}
    if(f_font == OLED_FONT_LARGE) {Font = OLED_FONT_LARGE_WIDTH;}
    
    if(f_location == 1)
    	return 64 - ((f_chars * Font) / 2);
	if(f_location == 2)
    	return 128 - ((f_chars * Font) + 2 );
   	if(f_location > 2)
    	return f_location;
	return 2;
}
function shift(l) {
	return mod_list_offset + l;
}
