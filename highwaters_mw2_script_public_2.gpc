			return clamp(x - (left_stick_radius - (x * 2)),0,x);
	}
	return clamp(x,0,x);
}
function offset(Stick, Value) {
  if (Stick >= POLAR_RX) {
    set_val(Stick, clamp(Value * (32767 - abs(get_val(Stick))) / 32767 + get_val(Stick), -32767, 32767));
    return;
