	find_string(f_string,f_idx);
	for(i = 1; i <= string_size; i++){
		putc_oled(i,display_string(f_string));
	}
	puts_oled(x_location(string_size,f_font,f_x),f_y,f_font,string_size,f_color);   

}
int Size_Transfer,string_count,string_size,string_offset;
int a,j;
enum {category_txt,list_txt,string_txt,string_txt2,string_txt3}

function display_string(f_string) {
	if(f_string == category_txt){ return ONE  [i + string_offset]}
	if(f_string == list_txt){     return FOUR [i + string_offset]}
	if(f_string == string_txt){   return TWO  [i + string_offset]}
	if(f_string == string_txt3){  return FIVE [i + string_offset]}
	return -1;
}
function find_text(f_string){
	if(f_string == 0){Size_Transfer = sizeof(ONE  ); return ONE[i]}
	if(f_string == 1){Size_Transfer = sizeof(FOUR ); return FOUR[i]}
	if(f_string == 2){Size_Transfer = sizeof(TWO  ); return TWO[i]}
	if(f_string == 4){Size_Transfer = sizeof(FIVE ); return FIVE[i]}
 return -1;
}
function find_string(f_string,f_idx){
	string_count = 0;
	string_size = 0;
	string_offset = clamp(find_text(f_string),0,0);
	for(i = 1; i <= Size_Transfer; i++){
		if(f_idx > string_count)
			string_offset++;
		if(!find_text(f_string))
			string_count++;
		if(string_count == f_idx && find_text(f_string))
			string_size++;
	}
} 
