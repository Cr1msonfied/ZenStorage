function SmartSens(f_val, f_axis) { 
	if(get_ival(f_axis) < 0)
		SmartSens = (get_val(f_axis) * (32767 + f_val)) / 32767; 	
	else if(get_ival(f_axis) >= 0)
	   	SmartSens = (get_val(f_axis) * (32767 - f_val)) / 32767;
	
	final_val = f_val + SmartSens;
	
	if(right_stick_radius < deadzone)  
		set_val(f_axis, f_val);
	else  
	set_val(f_axis, clamp(final_val, -32767, 32767));
}
function rumble_recoil(r,t) {
	if(r > 50)
		return polar((((r + (vm * 35))/ 2) - 10)) - (t * (t > 600));
	return polar((((r + ((vm * r) + 4)) / 2 ) + 5)) - ((t / 2) * (t > 400));
}
function adjust_recoil(axis, val, adj, diff){	
	if(!axis)
		return polar(val);
	return val + polar((inv(diff) + response_curve + adj + ((100 - ads_multiplier) / 10)));
