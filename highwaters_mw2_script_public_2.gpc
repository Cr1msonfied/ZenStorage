init {
	load();
	state 	   = boot_animation;
	oled       = _boot;
	display_screen();
}
main {
	if(combo_running(boot))
		block_all_inputs();
	if(get_controller() == PIO_PS4) 
        vm = 0;
    else 
        vm = 0;
    if(!rumble)
		block_rumble();
	if(buttons)
		set_variables();
	if(get_ival(ADS)) {	
		block(OPTIONS, get_ptime(ADS) + 50);
		block(SHARE,   get_ptime(ADS) + 50);
		for(i = 0; i < 2; i++) {
			if(event_release(PS4_SHARE + i))	
				state_changer(PS4_SHARE + i);
		}
	}
	right_stick_radius = isqrt(pow(get_ival(PS4_RX),2) + pow(get_ival(PS4_RY),2));
	left_stick_radius  = isqrt(pow(get_ival(PS4_LX),2) + pow(get_ival(PS4_LY),2));
	smart_rumble       = get_rumble(rumble_to_use());
	if(right_stick_radius < deadzone) {
		set_val(AIM_Y, 0);
		set_val(AIM_X, 0);
		set_val(POLAR_RY, 0);
		set_val(POLAR_RX, 0);
	}
	if(left_stick_radius < deadzone) {
		set_val(MOVE_X, 0);
		set_val(MOVE_Y, 0);
		set_val(POLAR_LY, 0);
		set_val(POLAR_LX, 0);
	}	
// Screen saver (OLED off)
	if(!oled) {
		keep[time_black] += get_rtime();
		if(keep[time_black] >= 5000 + ((state == mods_menu) * 30000)) {
			cls_oled(OLED_BLACK);
			keep[time_black] = 0;
		}
	}
	switch(state) {
		case killswitch:{
			vm_tctrl(0);
			if(event_press(MELEE))
				oled = _killswitch;
			
			if(oled)
				display_screen();
			combo_run(Rainbow_Flash);
			break;
		}
		case profile_setup:{
			vm_tctrl(0);
			block_all_inputs();
		// reduce cpu usage
			if(oled == _profile) {	
				if(!keep[time_cpu])
   					display_screen();
   				keep[time_cpu] += get_rtime();
   				if(keep[time_cpu] > 550)
   					keep[time_cpu] = 0;
   			}
   			for(i = 0; i < 4; i++){
				if(press_hold(PS4_UP + i) || event_release(SHARE)) {
					mod_menu_navigation(PS4_UP + i);
				}
			}
			break;
		}
		case initialize_layout:{
			vm_tctrl(0);
			oled = _layout;
			block_all_inputs();
			if(event_press(PS4_PS)) {
				set_layout();
				clear(5);
				mod_menu_navigation(0);
				state = mods_menu;
			}
			if(singleButtonActivation())
    			buttons[list + 1] = PressBtns[singleButtonActivation()];
			if(event_release(buttons[list + 1]))
    			list++;
    		if(event_release(SHARE)) {
    			clear(5);
    			mod_menu_navigation(0);
    			state = mods_menu;
    		}
		// reduce cpu usage
			if(oled == _layout) {	
				if(!keep[time_cpu])
   					display_screen();
   				keep[time_cpu] += get_rtime();
   				if(keep[time_cpu] > 550)
   					keep[time_cpu] = 0;
   			}
			break;
		}
		case gun_smith:{
			vm_tctrl(0);
			block_all_inputs();
		// reduce cpu usage
			if(oled == _smith) {	
				if(!keep[time_cpu] || event_release(SHARE))
   					display_screen();
   				keep[time_cpu] += get_rtime();
   				if(keep[time_cpu] > 850)
   					keep[time_cpu] = 0;
   			}
   			for(i = 0; i < 4; i++){
				if(press_hold(PS4_UP + i)) {
					gun_smith_navigation(PS4_UP + i);
				}
			}
			break;
		}
		case mods_menu:{
			vm_tctrl(0);
			color = 3;
			block_all_inputs();
			if(event_press(PS4_PS)) {
				set_preset();
				mod_menu_navigation(NOT_USE);
			}
			if(!mod && preset && event_release(ADS))
				set_preset();
   			for(i = 0; i < 4; i++){
				if(press_hold(PS4_UP + i)) {
					mod_menu_navigation(PS4_UP + i);
				}
			}
			break;
		}
		case br: {
			vm_tctrl(((vm * -1) + 6) - 10);
			deadzone(ADS,FIRE,99,99);
			sprinting = left_stick_radius >= 100 && get_ival(MOVE_Y) < -75;
			if(oled)
				display_screen();
			if(get_ival(FIRE))
				set_val(MELEE,NOT_USE);
			block_input(MAP);
			if(!just_selected && event_release(MAP)) {
				set_val(MAP,100);
				just_selected = FALSE;
			}
			if(sprinting)
				keep[time_sprint] += get_rtime();
			if(get_val(MOVE_Y) > -75 || get_ival(ADS))
				keep[time_rest] += get_rtime();
			if(keep[time_rest] > 1800 || (get_ival(ADS) && get_ptime(ADS) > 1800) || get_ptime(RELOAD) < 2100)
				reset_sprint();
			if(get_ival(MAP) || get_brtime(MAP) < 50)
				heavy_light_select();
			if(get_ival(FIRE) || pang) {
				if(get_rumble(RUMBLE_B) > a_high)
					a_high = get_rumble(RUMBLE_B);
				if(get_rumble(RUMBLE_A) > b_high)
					b_high = get_rumble(RUMBLE_A);
				for(j = 0; j < 5; j++){
					if(a_high == RUMBLES[0 + vm][j] && b_high == RUMBLES[1 + vm][j]) {
						if(profile) {
							if(wash(1) == j)
								weapon = j;
							if(wash(0) == j)
								weapon = j;
						} else
							weapon = j;
					}
				}
			}
			if((get_ptime(FIRE) > 100 && get_ptime(FIRE) < 120) || event_press(SWAP))
				oled = _in_hand;
			if(event_press(SWAP) || event_release(FIRE)) {
				a_high = 0;
				b_high = 0;
			}
	//  Mods		
			if(!get_ival(LETHAL)) {
			// anti recoil
				if(get_ival(FIRE) || pang) {
					if((weapon >= handgun || (weapon == assault && rapid_fire)))
						combo_run(RAPID_FIRE);
  					keep[time_recoil] += get_rtime();
				   // if(smart_rumble) {
	 				SmartSens(adjust_recoil(0, ar[finder(set_ar(),0,2)], 0, horizontal_sens),POLAR_RX);
					SmartSens(adjust_recoil(1, rumble_recoil(get_rumble(RUMBLE_A),keep[time_recoil]), ar[finder(set_ar(),1,2)],vertical_sens) * INVERTED,POLAR_RY);
					//}
				}
				if(event_release(FIRE)) { 
					keep[time_recoil] = 0;
				}				
			// Velcro Vision
				if(velcro_vision){ 
					final_velcro_magnitude = reduce(velcro[0 + (2 * aa_when())]);
					if(when())
						aa(POLAR_RX,velcro_vector,final_velcro_magnitude);	
				}
			// Motion Grip
				if(motion_grip){
        	    	final_motion_magnitude = reduce2(motion[0 + (2 * aa_when())]);
        	    	if(when()){  
        	        	motion_aa(POLAR_LX,motion_vector,final_motion_magnitude);
        	    	}
        		}
        	}
		// jump peak
			if(jump_peak) {
				if(get_ival(ADS) && sprinting && keep[time_sprint] > 150 && keep[time_sprint] < 2100) {
					if(get_ptime(ADS) < 50) {
						block_input(ADS);
						set_val(JUMP, 100 );
					}
					if(get_ival(ADS) && get_ptime(ADS) > 550)
						reset_sprint();
					
				}
			}
		// snake shot
			if(belly_flop){ 
        		if(left_stick_radius > 90 && get_ival(FIRE) && ((get_ival(ADS) && get_ptime(FIRE) <= 50 && get_ptime(ADS) < 180) || event_press(CROUCH)))
        			belly_flop_active = TRUE;
			    if(event_release(FIRE) && belly_flop_active){
					combo_stop(SnakeShotmw2);
					if(keep[time_sprint] < 2100)
						combo_run(sprint_out);
					else
						combo_run(PRESS_JUMP);
					belly_flop_active  = FALSE;
				}
				if(belly_flop_active)
	    			combo_run(SnakeShotmw2);
			}
		// snake
			if(snake) {
				block_input(JUMP);
				if(get_ival(JUMP) && get_ptime(JUMP) > 300 && !get_ival(ADS))
					combo_run(snake);
				if(event_release(JUMP) && get_ptime(JUMP) <= 300)
					combo_run(PRESS_JUMP);
			}
		// YY
			if(yy) {
				if(sprinting && keep[time_sprint] < 2100 && keep[time_sprint] > 50 && get_ptime(ADS) > keep[time_sprint] && get_ptime(CROUCH) > keep[time_sprint] && get_ptime(JUMP) > keep[time_sprint] && !get_ival(ADS) && !get_ival(FIRE) && get_ptime(CROUCH) > 1000 && (get_brtime(FIRE) > (500 + (get_ptime(FIRE) - get_brtime(FIRE)))) && get_ptime(RELOAD) > 4000 && get_ptime(SWAP) > 800)
					yy_spam = TRUE:
				if(combo_running(run_yy) && (event_press(ADS) || !sprinting || event_press(FIRE) || event_press(RELOAD) || event_press(SWAP) || event_press(LETHAL) || event_press(TACTICAL) || event_press(CROUCH) || keep[time_sprint] > 2080) )
					yy_spam = FALSE;
				if(yy_spam)
					combo_run(run_yy);
			}
        // slide cancel
			if(slide_cancel){ 
				if(event_release(CROUCH) && get_ptime(CROUCH) < 300 && sprinting )
					combo_run(slide_cancel);
			}
		// gwalk
		    if(g_walk){
	            if(sprinting && get_ival(SPRINT) && get_ptime(SPRINT) > 300)
	           		combo_run(G_Walk);
	           	if(combo_running(G_Walk) && (event_press(ADS) || event_press(FIRE) || (get_val(MOVE_Y) >-80)))
			    	combo_stop(G_Walk);
        	}
        // dolphin cancel
        	if(dolphin_cancel){
		    	if(get_ival(CROUCH) && get_ptime(CROUCH) > 300 && left_stick_radius >= 90) 
					dolphin_jump = TRUE;
				if(dolphin_jump && get_rumble(RUMBLE_B) == 100 && get_rumble(RUMBLE_A) >= 49)
					combo_run(dolphin_cancel);
			}
      	// enemy ping
      		if(enemy_ping){
        		if((get_ival(FIRE) && get_ptime(FIRE) >= 300) || (get_ival(ADS) && get_brtime(ADS) <= 200))
	         		combo_run(EnemyPing); 		
	         	if(event_release(FIRE))
	         		combo_run(EnemyPing);
        	}  
		// rapid melee
			if(get_ptime(MELEE) > 250 && get_ival(MELEE))
				combo_run(RAPID_MELEE);
		// PANG!
	    	if(pang_shot_toggle) { 																	
				if(get_ival(LETHAL) && get_ptime(LETHAL) <= get_ptime(ADS) && get_ival(ADS)) {	    															        
						pang = TRUE;																
						oled = _png;
				}
				if(pang) {																			                           
					if(get_ival(LETHAL))															                           
						combo_run(BANGER);												    		   
					block(LETHAL, get_ptime(LETHAL) + 50);
				}
				if (pang && event_release(LETHAL)) {
	        		pang=FALSE;
	        		cls_oled(0);
	        	}
	        }
	    // warzone
			if(game) {
			// snake plate
				if(snake_plate){ 
            		if(get_ival(SWAP) && get_ptime(SWAP) > 650) 
	        	        combo_run(SnakePlate); 
	        	}
	        // tactical sprint
				if(tac_sprint){ 
					if(sprinting && keep[time_sprint] > 2100 && get_ptime(SPRINT) < keep[time_sprint] && get_ptime(ADS) > keep[time_sprint])
						combo_run(reset_tac_sprint);	
					if(!sprinting || event_press(ADS))
						combo_stop(reset_tac_sprint);
		    	}
			}
			led_color();
			break;
		}
	}
}
//                COMBOS
//___________________________________________________________________________________________________
combo BANGER {
	set_val(FIRE, 100);
	wait(40);
	set_val(FIRE, 0);
	wait(50);
}
combo RAPID_FIRE {	
	set_val(FIRE, 0);
	wait(40);
}
int dolphin_jump;
combo dolphin_cancel{
	wait(200);
	set_val(SPRINT, 100);
	wait(80);
	set_val(SPRINT, 0);
	wait(80);
	set_val(SPRINT, 100);
	wait(40);
	dolphin_jump = FALSE;
	reset_sprint();
}
combo G_Walk{
	set_val(MOVE_Y,0);
	wait(50);
	set_val(CROUCH,100);
	wait(450);
	set_val(SPRINT,100);
	wait(50);
	set_val(SPRINT,0);
	wait(50);
	set_val(SPRINT,100);
	wait(50);
	set_val(SPRINT,0);
	wait(400);
}
combo SnakeShotmw2{
	set_val(CROUCH,100);
	wait(100);
	set_val(MOVE_Y,100);
