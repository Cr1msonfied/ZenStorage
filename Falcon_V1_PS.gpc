*/ 
int ModDigit;
int TaylorRadius     = 15;
int TaylorAngle      = 15;
int TaylorMinRadius  = 5;
int PolarRadius      = 15;
int PolarAngle       = 15;
int PolarRadiusBoost = 5;
int PolarAngleBoost  = 10;
int PolarRelease     = 20;
int LegacyAR1RY      = 28;
int LegacyAR1RX      = 0;
int ProneShotWait    = 50;
int JumpShotWait     = 570;
int CrouchShotWait   = 150;
int StrafeWait       = 1500;
int RapidFireHold    = 30;
int RapidFireWait    = 30;
