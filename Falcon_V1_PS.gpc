int PolarVIAssist     =  On;
int PolarVIBoost      =  On;
int LegacyRecoil      =  On;
int ProneShot         = Off;
int CrouchShot        = Off;
int JumpShot          = Off;
int StrafeShot        = Off;
int RapidFire         = Off;
int SlideCancelCW     = Off;
int SlideCancelMW     =  On;
int AutoPing          = Off;
