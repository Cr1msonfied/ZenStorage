function anti_recoil(){


return weapon_start[profile_idx] + (profile_start[Adjustable_Value[0]] * 455) 
}


function x_location(f_chars,f_font,f_location) {

	if(f_font == OLED_FONT_SMALL) {Font = OLED_FONT_SMALL_WIDTH;}	    
    if(f_font == OLED_FONT_MEDIUM){Font = OLED_FONT_MEDIUM_WIDTH;}
    if(f_font == OLED_FONT_LARGE) {Font = OLED_FONT_LARGE_WIDTH;}
    
    if(f_location == 1){  
    	return 64 - ((f_chars * Font) / 2);
   	}
	
	if(f_location == 2){  
    	return 128 - ((f_chars * Font) + 3 ) ;
   	}
	if(f_location > 2){  
    	return f_location;
   	}
	
	return 3;     
	
}


function cycle(f_val, f_lo, f_hi) {

	if(f_val > f_hi) return f_lo;
	if(f_val < f_lo) return f_hi;
	return f_val;

}

function Cycle(f_val,f_bmin,f_bmax,f_kmin,fk_max,f_min,f_max){
	if(event_press(f_bmin) || key_press(f_kmin)){
		f_val = cycle(f_val - 1, f_min ,f_max)
		Display = TRUE;
	}
	if(event_press(f_bmax) || key_press(fk_max) ){
		f_val = cycle(f_val + 1, f_min ,f_max)
		Display = TRUE;
	}
	return f_val
}

function line_count(f_val){
	if(event_press(PS4_DOWN) && f_val < 2 || key_press(Menu_Down) && f_val < 2  ){
		f_val++
	}		
		
	if(event_press(PS4_UP) && f_val > 0 || key_press(Menu_Up) && f_val > 0  ){
		f_val--
	}

return f_val

}

function line_check(f_val){
	if(event_press(PS4_UP) && !f_val || key_press(Menu_Up) && !f_val){
		cycle = TRUE
	}
	if(event_press(PS4_DOWN) && f_val == 2 || key_press(Menu_Down) && f_val  == 2){
		cycle = TRUE
	}

}

function extract (f_sight,f_idx,f_val){
	if(Shift[2] == 8 && Shift[3] == 8){
		if(f_sight == 2){
				Shift = 16;
			}
			else{ 
				Shift = 0;
			}
		}
		else{
			Shift = (f_sight + 1) << f_idx
		
		}

		if(f_idx == 0){
			if(f_val >> Shift & 0x80){
				return (~(f_val >> Shift & 0x7F)) + 1
				}
			if(f_val >> Shift & 0x7F){
				return f_val >> Shift & 0x7F
				}
			}
		if(f_idx == 1){
			return f_val >> (8 + Shift) & 0x7F
		}	
	
	
	
return 0
}

function mod_extract (f_idx,f_val){
		if(f_idx == 0 || f_idx == 1){
			if(f_val >> SHIFT[f_idx] & 0x400){
				return (~(f_val >> SHIFT[f_idx] & 0x3FF)) + 1
				}
			else{
				return f_val >> SHIFT[f_idx] & 0x3FF
				}
			}
		if(f_idx == 2 || f_idx == 3 ){
			return f_val >> SHIFT[f_idx] & 0x1F
		}	
return 0
}

function signed_8bit_unpacked(f_val,f_location){
	if(f_location){
		Shift = 8;
	}
	else{
		Shift = 0
	}
	if(f_val >> Shift & 0x80){
		return (~(f_val >> Shift & 0x7F)) + 1
	}

		return f_val >> Shift & 0x7F
	
}

		 




