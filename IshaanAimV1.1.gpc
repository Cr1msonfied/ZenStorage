function correctAngle(angle){
if(angle < 0) 
angle = 360 + (angle % 360);
angle = (angle + 90) % 360;
return angle;
}
function sin(angle){
return dint8(correctAngle(angle));
}
function cos(angle){
return dint8((correctAngle(angle) + 90) % 360);
}


