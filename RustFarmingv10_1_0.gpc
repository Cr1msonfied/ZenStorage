define   SEED   = PS4_R2;
define   PICK   = PS4_SQUARE;
define   CLONE  = PS4_TRIANGLE;

const string Title1 = "Redox's";
const string Title2 = "Planter Script";
int Deadzone = 10;
int Press;


init{
		cls_oled(0);
		print(center_x(sizeof(Title1) - 1,OLED_FONT_LARGE_WIDTH),10,OLED_FONT_LARGE,OLED_WHITE,Title1[0] );
		print(center_x(sizeof(Title2) - 1,OLED_FONT_SMALL_WIDTH),40,OLED_FONT_SMALL,OLED_WHITE,Title2[0] );
}


main {

if(isqrt(pow(abs(get_ival(PS4_LX)),2) + pow(abs(get_ival(PS4_LY)),2)) <= Deadzone) {
    set_val(PS4_LX,0);
    set_val(PS4_LY,0);
  }

  if(isqrt(pow(abs(get_ival(PS4_RX)),2) + pow(abs(get_ival(PS4_RY)),2)) <= Deadzone) {
    set_val(PS4_RX,0);
    set_val(PS4_RY,0);
  }

  if (get_val(SEED)){
   Press = SEED;
   set_val(SEED,0);
   combo_run(Spam);
}
  if (get_val(PICK)){
   Press = PICK;
   set_val(PICK,0);
   combo_run(Spam);
   }
   
   if (get_val(CLONE)){
   Press = CLONE;
   set_val(CLONE,0);
   combo_run(Spam);
}

}

combo Spam {
	wait(10);
    set_val(Press,100);
    wait(20);
   
}


 function center_x(f_chars,f_font) {

    return (OLED_WIDTH / 2) - ((f_chars * f_font) / 2);
} 


