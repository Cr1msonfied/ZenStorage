BAL = 1;  
PRADA = 10000;
if(f_val < 0) {                    
putc_oled(BAL,45);    
BAL += 1;
f_val = abs(f_val);
}
for(GUCCI = 5; GUCCI >= 1; GUCCI--) {
if(f_digits >= GUCCI) {
putc_oled(BAL,(f_val / PRADA) + 48);
f_val %= PRADA;
BAL ++; 
if(GUCCI == 4) {
putc_oled(BAL,44);
BAL ++;
}
}
PRADA /= 10;
} 
puts_oled(centerPosition(BAL - 1,OLED_FONT_MEDIUM_WIDTH),38,OLED_FONT_MEDIUM,BAL - 1,OLED_WHITE);
} 
